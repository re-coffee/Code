from cryptography.fernet import Fernet
import datetime

class Senha():
    def __init__(self, valor):
        self.valor = valor
        self.chave = Fernet.generate_key() #Salvar em local seguro

    def Criptografar(self):
        return Fernet.encrypt(self.valor.encode(), self.chave)

    def Decriptografar(self, valor):
        return 'a'

